{
  "name": "t3awanu-2",
  "version": "1.0.0",
  "description": "Full-stack application with separate frontend and backend",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "build:blog-module": "cd packages/react-blog-module && npm run build",
    "install:all": "npm install && npm run install:frontend && npm run install:backend && npm run install:blog-module",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:blog-module": "cd packages/react-blog-module && npm install",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && npm test",
    "debug": "concurrently \"npm run debug:frontend\" \"npm run debug:backend\"",
    "debug:frontend": "cd frontend && npm run dev -- --debug",
    "debug:backend": "cd backend && npm run dev:debug",
    "publish:blog-module": "cd packages/react-blog-module && npm run build && npm publish"
  },
  "devDependencies": {
    "@types/multer": "^1.4.12",
    "@types/uuid": "^10.0.0",
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "multer": "^1.4.5-lts.2",
    "uuid": "^11.1.0"
  }
}
